{{define "content"}}
<h2>{{.Table}} Data Analysis</h2>
        <p>Limited to most common 100 values per table</p>

    <p>
        Jump to:
{{range .Analysis}}
<a href="#col_{{.Column}}">{{.Column}}</a>
{{end}}
    </p>

<div>
{{range .Analysis}}
    <h3 id="col_{{.Column}}">{{.Column}}</h3>
    <table class="data-table-view">
        <thead>
        <tr>
            <th>Value</td>
            <th>Count</td>
        </tr>
        </thead>
        <tbody>
    {{range .ValueCounts}}
        <tr>
            <td>{{.Value}}</td>
            <td>{{.Quantity}}</td>
        </tr>
    {{end}}
        </tbody>
    </table>
{{end}}
</div>
{{end}}
